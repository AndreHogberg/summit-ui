@namespace SummitUI.Docs.Design

@switch (Level)
{
    case SuHeadingLevel.H1:
        <h1 class="@FinalClass" @attributes="OtherAttributes">@ChildContent</h1>
        break;
    case SuHeadingLevel.H2:
        <h2 class="@FinalClass" @attributes="OtherAttributes">@ChildContent</h2>
        break;
    case SuHeadingLevel.H3:
        <h3 class="@FinalClass" @attributes="OtherAttributes">@ChildContent</h3>
        break;
    case SuHeadingLevel.H4:
        <h4 class="@FinalClass" @attributes="OtherAttributes">@ChildContent</h4>
        break;
    case SuHeadingLevel.H5:
        <h5 class="@FinalClass" @attributes="OtherAttributes">@ChildContent</h5>
        break;
    case SuHeadingLevel.H6:
        <h6 class="@FinalClass" @attributes="OtherAttributes">@ChildContent</h6>
        break;
}
